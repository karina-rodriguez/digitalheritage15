@inproceedings{	C34-P9233,
	author = {Torsten Ullrich and Dieter W. Fellner},
	title = {Robust Shape Fitting and Semantic Enrichment},
	year = {2008},
	booktitle = {CIPA 2007},
	pages = {6},
	keywords = {Algorithms, Classifications, Geometry, Mathematics, Point clouds,
Reconstruction, Registration},
	pubkey = {C34-P-19233},
	abstract = {A robust fitting and reconstruction algorithm has to cope with two major
problems. First of all it has to be able to deal with noisy input data and
outliers. Furthermore it should be capable of handling multiple data set
mixtures.   

The decreasing exponential approach is robust towards outliers and multiple
data set mixtures. It is able to fit a parametric model to a given point
cloud. As parametric models use a description which may not only contain a
generative shape but information about the inner structure of an object,
the
presented approach can enrich measured data with an ideal description. This
technique offers a wide range of applications.},
}
