@INPROCEEDINGS{ullrich_robust_2007,
  author = {Ullrich, Torsten and Fellner, Dieter W.},
  title = {Robust Shape Fitting and Semantic Enrichment},
  booktitle = {International Workshop on e-Documentation and Standardisation in
	Cultural Heritage (CIPA)},
  year = {2007},
  address = {Athens, Greece},
  month = oct,
  abstract = {A robust fitting and reconstruction algorithm has to cope with two
	major problems. First of all it has to be able to deal with noisy
	input data and outliers. Furthermore it should be capable of handling
	multiple data set mixtures. The decreasing exponential approach is
	robust towards outliers and multiple data set mixtures. It is able
	to fit a parametric model to a given point cloud. As parametric models
	use a description which may not only contain a generative shape but
	information about the inner structure of an object, the presented
